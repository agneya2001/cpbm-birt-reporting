            <xml-property name="queryText"><![CDATA[
SELECT 
cloud_portal.usage_data.usage_type, 
 cloud_portal.usage_data.resource_type,
sum(cloud_portal.usage_data.raw_number)

FROM cloud_portal.usage_data, cloud_portal.users

where cloud_portal.users.username= ? and cloud_portal.users.id =  cloud_portal.usage_data.user_id 
and cloud_portal.usage_data.start_date >= ? and cloud_portal.usage_data.start_date <= ?

group by cloud_portal.usage_data.usage_type;]]></xml-property>
